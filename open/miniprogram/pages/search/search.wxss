}

.other-navigator-hover{
  background:#fff;
}

.scroll_left{
  width:25%;
  height:100%;
  background:#f0f0f0;
  text-align:center; 
  position: fixed;
  left: 0;top: 97rpx
}
.mintext{
font-size:14px;
width:30%;
float:left;
margin-top:6px;
color:#333;
