/*new css*/
.user-header {position: relative; padding: 235rpx 0 0; text-align: center; background-color: #fff; margin-bottom: 20rpx;}
.user-header-bg {position: absolute; top: -1px; left: 0; width: 100%; height: 325rpx; background-color: #fe1b44;
z-index: 10;}
.user-header-bg image {position: absolute; bottom: 0; left: 0; width: 100%;}
.user-header-con {position: relative; z-index: 20;}
.user-header-photo {width: 120rpx; height: 120rpx; border: 5rpx solid #fff; border-radius: 50%; margin: 0 auto;}
.user-header-name {font-family: 32rpx; font-weight: 700; color: #000; line-height: 50rpx;}

.user-header-right {position: absolute; top: 0; right: 0; width: 200rpx; height: 80rpx;}
.user-header-icon {float: right; width: 80rpx; height: 40rpx;}
.user-header-icon image {width: 40rpx; height: 40rpx; margin: 0 auto;}

.user-num-menu {display: flex; box-sizing: border-box; padding: 50rpx 15rpx;}
.user-num-view {box-sizing: border-box; width: 33.33%; padding: 0 15rpx; color: #fff; font-size: 24rpx;
line-height: 30rpx;}
.user-num-view .bg {border-radius: 10rpx; padding: 20rpx 0; text-align: center;}
.user-num-view .n-num {padding-bottom: 5rpx;}
.user-num-view .n-num text {font-size: 32rpx; font-weight: 700;}

.user-area {background-color: #fff; margin-bottom: 20rpx;}
.user-title {height: 80rpx; line-height: 80rpx; padding: 0 30rpx;}
.user-title text {font-size: 32rpx; font-weight: 700; color: #000;}
.user-title .n-link {float: right; font-size: 24rpx; color: #a6a6a6; padding-right: 30rpx; position: relative;}
.user-title .n-link image {width: 24rpx; height: 24rpx; position: absolute; top: 50%; right: 0; margin-top: -12rpx;}

.user-order-menu {padding: 20rpx 15rpx;}
.user-order-view {float: left; width: 33.33%; height: 180rpx; box-sizing: border-box; text-align: center;}
.user-order-view image {width: 100rpx; height: 100rpx; border-radius: 50%; margin: 0 auto;}
.user-order-view .n-text {font-size: 24rpx; font-weight: 700; color: #000; line-height: 32rpx;}

.user-menu-area {padding: 20rpx 15rpx;}
.user-menu-view {float: left; width: 20%; height: 120rpx; box-sizing: border-box; text-align: center;}
.user-menu-view image {width: 50rpx; height: 50rpx; margin: 0 auto;}
.user-menu-view .n-text {font-size: 24rpx; font-weight: 700; color: #000; line-height: 32rpx;}

.user-article-area {padding: 20rpx 30rpx;}
.user-article-area navigator {height: 80rpx; border-bottom: 1px solid #eee; line-height: 80rpx; position: relative;}
.user-article-area navigator:last-child {border: 0;}
.user-article-area .n-title {font-size: 32rpx; font-weight: 700; color: #000;}