/*地址管理*/

page {
  background-color: #efeff4;
  font-size: 10pt;
  -webkit-user-select: none;
  user-select: none;
  width: 100%;
  overflow-x: hidden;
}
.address_text{
  position: absolute;
  top: 2px;
  left:30px;
  color: #FF6347;
}
.add_icon{
  padding-top: 8px;
  height: 30px!important;
}
.address_icon{
  height: 30px!important;
}
.heji{
  padding-left:30rpx;
  position: relative
}

.address_div{
  margin-right: 34rpx;
  position: relative;
}
.heji_icon{
  padding-top:10px;
  height: 30px;
}
.heji_text{
  position: absolute;
  top: 0;
  left:45px;
  font-size:34rpx;
  height: 45px;
  line-height: 45px
}

.address_img_div{
  position: absolute;
  top: 0;
  right: -34rpx;
  width: 34rpx;
  height: 100%;
  display: flex

}
.address_img{
  width: 34rpx;
  height: 34rpx;
 margin: auto
}

.address {
  display: flex;
  flex-wrap: wrap;
  background-color: #fff;
  margin-top: 1px;
  
  margin: 0rpx 30rpx 30rpx 30rpx;
}

.top-ql-1{
  float:right;
}

