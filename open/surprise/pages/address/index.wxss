.address-icon {
  position: absolute;
  top: 0;
  left: 0;
  height: 100%;
  display: flex;
  width: 250px
}

.top-ql{
  text-align: right;
  padding-right:30rpx;
  height: 40px;
  line-height: 40px
}
 
 
radio .wx-radio-input.wx-radio-input-checked{
      background-color:red;
}

radio .wx-radio-input.wx-radio-input-checked{
   border: none;
   background: #FF6347; 
}
checkbox .wx-checkbox-input.wx-checkbox-input-checked{
    background: #FF6347
}



.address-detail {
  width: 100%;
  padding: 30rpx 30rpx 0 30rpx;
}

.address-name-phone {
  margin-bottom: 20rpx;
  font-size: 11pt;
  /* font-weight: 900; */
}

.address-name{
  margin-right: 20rpx;
}

.address-info {
  margin-bottom: 30rpx;
}

.address-edit {
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
  border-top: 1px #efeff4 solid;
  height: 45px;
  line-height: 45px;
  position: relative
}

.add-address {
  display: flex;
  align-items: center;
  margin-top: 20px;
  height: 45px;
  /* padding: 18rpx; */
  background-color: #fff;
  font-size: 11pt;
  /* font-weight: 900; */
  justify-content:center;
border-radius:5px;

}

.add-address-icon {
  height: 20px;
  width: 20px;
}
.icon-tianjia::before {
  margin-right:5px; 
}

.add_dell{
  position:fixed;
  bottom: 0;
  left: 0;
  height: 45px;
  line-height: 45px;
  width: 100%;
  background-color: white;
  border-top: 1px solid #eee;
  z-index: 99
}

.add_left{
  width: 40%;
  float: left
}
.add_left .address-icon{
  top:-4rpx;
  left:30rpx;
}

.add_word{
  font-size:34rpx;
  line-height: 45px;
  padding-top:1px;

}

.add_right{
  width: 30%;
  float: right;
  text-align: center;
  background-color: #FF6347;
  font-size:34rpx;
  letter-spacing: 1px;
}