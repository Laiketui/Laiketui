.datails-right-icon {position: absolute; top: 30rpx; right: 30rpx; width: 200rpx;}
.datails-icon-box {float: right; width: 100rpx; text-align: center;}
.datails-icon-image {width: 40rpx; height: 40rpx; margin: 0 auto;}
.datails-icon-text {font-size: 24rpx; line-height: 32rpx; color: #000;}

.datails-size-area {background-color: #fff; padding: 0 30rpx; font-size: 28rpx; color: #000; line-height: 90rpx;
height: 90rpx; position: relative; text-align: left; margin-bottom: 20rpx;}
button[type=default].datails-size-area {background-color: #fff;}

/*groups-list-ing css*/
.groups-list-ing {background-color: #fff; margin-bottom: 20rpx; padding:  0 30rpx;}
.groups-list-ing .n-title {line-height: 80rpx; font-size: 24rpx;}
.groups-list-ing .n-pt {padding: 30rpx 0; color: #666; font-size: 28rpx; border-top: 1px solid #eee;}

